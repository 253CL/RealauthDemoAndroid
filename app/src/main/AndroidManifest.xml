<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chuanglan.alive.demo">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/demo_logo"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/demo_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.CLSDKCoreDemo"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activity.HomeActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.OnlineAliveDetectedActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.UploadIdCardActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.RealPeopleVerifyActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.RealPeopleVerifySuccess"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.RealPeopleVerifyFailed"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.OnlineAliveDetectedSuccessActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.IdCardOcrResultActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.IdCardOcrTestActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.TakePhotoActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.OnlineAliveDetectedTestActivity"
            android:exported="false"
            android:screenOrientation="portrait" />

        <provider
            android:name=".provider.CustomFileProvider"
            android:authorities="${applicationId}.FileProvider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>
    </application>

</manifest>